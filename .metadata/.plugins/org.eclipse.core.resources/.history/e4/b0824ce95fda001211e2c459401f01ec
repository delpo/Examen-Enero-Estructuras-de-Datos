package ej4jun12;

import java.util.LinkedList;

public class ArbolGeneral<T> {
	private T raiz ;
	private LinkedList < ArbolGeneral <T > > hijos ;

	public ArbolGeneral(){
		
	}
	
	public ArbolGeneral(T raiz,  LinkedList < ArbolGeneral <T > > hijos){
		this.raiz = raiz;
		this.hijos = hijos;
	}
	
}
