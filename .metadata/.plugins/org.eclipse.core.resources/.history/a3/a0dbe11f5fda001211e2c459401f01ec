package ej4jun12;

import java.util.ArrayList;

public class ProbadorGradoMax {
	ArrayList<ArbolGeneral<Integer>> hijos = new ArrayList<ArbolGeneral<Integer>>();
	ArrayList<ArbolGeneral<Integer>> hijos2 = new ArrayList<ArbolGeneral<Integer>>();
	ArbolGeneral<Integer> misubarbol = new ArbolGeneral<Integer>(12, hijos);
	ArbolGeneral<Integer> misubarbol2 = new ArbolGeneral<Integer>(12, hijos2);
	hijos.add(hijos);
	hijos.add(hijos2);
	hijos.add();
	ArbolGeneral<Integer> arbol = new ArbolGeneral<Integer>(27, hijos);
}
