package ej4jun12;

import java.util.LinkedList;

public class ArbolGeneral<T> {
	private T raiz ;
	private LinkedList < ArbolGeneral <T > > hijos ;

	public ArbolGeneral(){
		
	}
	
	public ArbolGeneral(T raiz,  LinkedList < ArbolGeneral <T > > hijos){
		this.setRaiz(raiz);
		this.hijos = hijos;
	}

	public T getRaiz() {
		return raiz;
	}

	public void setRaiz(T raiz) {
		this.raiz = raiz;
	}
	
}
